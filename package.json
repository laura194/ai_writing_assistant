{
  "name": "ai_writing_assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run dev --prefix backend\"",
    "build:pandoc:local": "docker buildx build --load -t aylin/pandoc-latex:latest -f docker-files/Dockerfile.pandoc .",
    "build:pandoc:push": "docker buildx build --platform linux/amd64,linux/arm64 -t aylin/pandoc-latex:latest -f docker-files/Dockerfile.pandoc . --push",
    "test:pandoc": "docker run --rm aylin/pandoc-latex:latest --version",
    "clean:pandoc": "docker rmi aylin/pandoc-latex:latest"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.31.1",
    "@emailjs/browser": "^4.4.1",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@react-three/drei": "^10.4.4",
    "@react-three/fiber": "^9.2.0",
    "@splinetool/react-spline": "^4.0.0",
    "@types/express": "^5.0.3",
    "aos": "^3.0.0-beta.6",
    "boxicons": "^2.1.4",
    "cors": "^2.8.5",
    "dictionary-de": "^3.0.0",
    "dictionary-en": "^4.0.0",
    "docx": "^9.5.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.0",
    "franc": "^6.2.0",
    "latex.js": "^0.12.6",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.511.0",
    "maath": "^0.10.8",
    "nspell": "^2.1.5",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.0.0",
    "react-parallax-tilt": "^1.7.299",
    "react-router-dom": "^7.5.3",
    "textarea-caret-position": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@stryker-mutator/core": "^9.1.1",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.18.6",
    "@types/nspell": "^2.1.6",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "msw": "^2.11.1",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  }
}
